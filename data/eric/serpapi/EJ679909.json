6", "publication_info": {"summary": "S Davies - Sociology of Education, 2002 - JSTOR", "authors": [{"name": "S Davies", "link": "https://scholar.google.com/citations?user=I9d5bD4AAAAJ&hl=en&oi=sra", "serpapi_scholar_link": "https://serpapi.com/search.json?author_id=I9d5bD4AAAAJ&engine=google_scholar_author&hl=en", "author_id": "I9d5bD4AAAAJ"}]}, "inline_links": {"serpapi_cite_link": "https://serpapi.com/search.json?engine=google_scholar_cite&hl=en&q=R1adqxZUzHAJ", "cited_by": {"total": 97, "link": "https://scholar.google.com/scholar?cites=8127963883842131527&as_sdt=2005&sciodt=0,5&hl=en", "cites_id": "8127963883842131527", "serpapi_scholar_link": "https://serpapi.com/search.json?as_sdt=2005&cites=8127963883842131527&engine=google_scholar&hl=en"}, "related_pages_link": "https://scholar.google.com/scholar?q=related:R1adqxZUzHAJ:scholar.google.com/&scioq=The+Paradox+of+Progressive+Education:+A+Frame+Analysis.+Davies,+Scott+Sociology+of+Education&hl=en&as_sdt=0,5", "serpapi_related_pages_link": "https://serpapi.com/search.json?as_sdt=0%2C5&engine=google_scholar&hl=en&q=related%3AR1adqxZUzHAJ%3Ascholar.google.com%2F", "versions": {"total": 6, "link": "https://scholar.google.com/scholar?cluster=8127963883842131527&hl=en&as_sdt=0,5", "cluster_id": "8127963883842131527", "serpapi_scholar_link": "https://serpapi.com/search.json?as_sdt=0%2C5&cluster=8127963883842131527&engine=google_scholar&hl=en"}}}]}